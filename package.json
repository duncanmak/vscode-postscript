{
    "name": "postscript",
    "displayName": "PostScript Language",
    "description": "Integrating PostScript Syntax highlighting",
    "version": "1.0.0",
    "publisher": "maxibanki",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maxibanki/vscode-postscript.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "postscript",
                "aliases": [
                    "Postscript",
                    "postscript"
                ],
                "extensions": [
                    ".ps",
                    ".eps"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "postscript",
                "scopeName": "source.postscript",
                "path": "./syntaxes/postscript.tmLanguage"
            }
        ]
    }
}